@{
    ViewBag.Title = "User Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">User Profile</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    User Profile
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12">

                            <div class="form-group">
                                <label>UserName</label>
                                <input class="form-control" id="UserName" value="@GlobalSettings.oUserMaster.UserName" disabled>

                            </div>
                           
                            <div class="form-group">
                                <label>Full Name</label>
                                <input class="form-control" placeholder="Full Name" id="FullName" value="@GlobalSettings.oUserMaster.FullName">
                            </div>

                            <div class="form-group">
                                <label>Mobile No.</label>
                                <input class="form-control" placeholder="Mobile No." id="MobileNo" value="@GlobalSettings.oUserMaster.MobileNo">
                            </div>

                            <div class="form-group">
                                <label>Email Id</label>
                                <input class="form-control" placeholder="Email Id" id="EmailId" value="@GlobalSettings.oUserMaster.EmailId">
                            </div>

                            

                            @*<button type="submit" class="btn btn-default">Submit Button</button>
                            <button type="reset" class="btn btn-default">Reset Button</button>*@

                        </div>
                        <!-- /.col-lg-6 (nested) -->
                        <!-- /.col-lg-6 (nested) -->
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
</div>
<!-- /#page-wrapper -->
    </div>
